source /usr/share/byobu/keybindings/common
escape ^Tt
register r "^t:source $HOME/.byobu/profile^M"		#     | Goes with F5 definition
register t "^tA^aa^k^h"					#     | Goes with the F8 definition
register s "^t[g G$>^h"					# Goes with ^a~ definition
register d "^t:source $BYOBU_PREFIX/share/byobu/keybindings/screen-escape-keys^M"
register r "^t:source $HOME/.byobu/profile^M"
register e "^t:source $BYOBU_PREFIX/share/byobu/keybindings/f-keys^M"
